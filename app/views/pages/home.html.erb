
<div class='landing' ng-controller='IndexCtrl as ic' ng-init='initialSet()'>
  <div class='landing ruby-background'>
    <div class='with-search-form'>
      <div class='header-content'>
       <center>
        <h1 class='header-title'>
          RubyRubyRuby
        </h1>
        <h3 class="header-subtitle">Tag line for brand goes here</h3>
      </center>

      <br><br>
      <form class="form-inline ">
        <div class="form-group">
          <input type="text" class="form-control navbar-wagon-search-input" id="exampleInputName2" placeholder="Search something">
        </div>
        <div class="form-group">
          <input type="email" class="form-control navbar-wagon-search-input" id="exampleInputEmail2" placeholder="Location">
        </div>
        <button type="submit" class="btn btn-default button-yellow  ">Search</button>
      </form>

    </div>
  </div>
</div>
</div>

</div>

