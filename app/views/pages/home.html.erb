<div class='landing' ng-controller='IndexCtrl as ic' ng-init='initialSet()'>
  <div class='landing ruby-background'>
    <div class='with-search-form'>
      <div class='headerBlock'>
       <center>
        <h1 class='headTitle'>
          RubyRubyRuby
        </h1>
      </center>
      <div class='hero-unit'>
       <form action="searchform" class="navbar-wagon-search">
        <input type="text" class="navbar-wagon-search-input" placeholder="Search something">
        <button type="submit" class="navbar-wagon-search-btn">
          <i class="fa fa-search"></i>
        </button>
      </form>
    </div>
    <div class='hero-unit'>
      <form action="searchform" class="navbar-wagon-search-2">
        <input type="text" class="navbar-wagon-search-input" placeholder="Location?">
        <button type="submit" class="navbar-wagon-search-btn">
          <i class="fa fa-search"></i>
        </button>
      </form>
    </div>
  </div>
</div>
</div>
</div>

